{
  "name": "mini",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@4.1.1",
  "scripts": {
    "dev": "yarn with-env next dev",
    "build": "cd ../.. && yarn build && cd apps/mini && yarn next:build",
    "next:build": "yarn with-env next build",
    "start": "yarn with-env next start",
    "serve": "NODE_ENV=production yarn with-env next start --port 8151",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "with-env": "dotenv -e ../../.env -c --",
    "check:type": "tsc --outDir /tmp",
    "check:type:watch": "yarn check:type -w"
  },
  "dependencies": {
    "@coinbase/onchainkit": "^0.38.16",
    "@my/ui": "*",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/supabase-js": "^2.48.1",
    "@tamagui/next-theme": "^1.131.3",
    "@tamagui/react-native-svg": "^1.131.3",
    "@tanstack/react-query": "4.18.0",
    "@trpc/server": "^10.45.2",
    "app": "*",
    "next": "15.3.2",
    "raf": "^3.4.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "vercel": "latest",
    "viem": "^2.31.7",
    "wagmi": "^2.15.6",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "15.3.2",
    "@next/eslint-plugin-next": "15.3.2",
    "@tamagui/next-plugin": "^1.131.3",
    "@types/node": "^18.6.4",
    "dotenv-cli": "^8.0.0"
  }
}
