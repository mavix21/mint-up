{
  "name": "@my/backend",
  "version": "0.0.0",
  "description": "Convex backend",
  "scripts": {
    "dev": "convex dev",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "setup": "convex dev --until-success",
    "typecheck": "tsc --noEmit"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "convex": "^1.26.1",
    "convex-helpers": "^0.1.104",
    "@neynar/nodejs-sdk": "^3.34.0"
  },
  "devDependencies": {
    "typescript": "~5.8.3"
  },
  "exports": {
    "./_generated/*": {
      "types": "./convex/_generated/*.d.ts",
      "default": "./convex/_generated/*.js"
    },
    "./nextjs": {
      "types": "./convex/nextjs.ts",
      "default": "./convex/nextjs.ts"
    },
    "./react": {
      "types": "./convex/react.ts",
      "default": "./convex/react.ts"
    },
    "./server": {
      "types": "./convex/server.ts",
      "default": "./convex/server.ts"
    },
    "./neynar": {
      "types": "./neynar.ts",
      "default": "./neynar.ts"
    }
  }
}
